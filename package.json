{
	"name": "mood-survey-server",
	"version": "1.0.0",
	"main": "index.js",
	"license": "MIT",
	"devDependencies": {
		"@types/better-sqlite3": "^7.6.3",
		"@types/express": "^4.17.15",
		"@types/luxon": "^3.2.0",
		"@types/node": "^18.11.18",
		"concurrently": "^7.6.0",
		"ts-node-dev": "^2.0.0",
		"typescript": "^4.9.4"
	},
	"scripts": {
		"install:dep": "yarn install && cd client && yarn install",
		"server:dev": "ts-node-dev --respawn --transpile-only src/app.ts",
		"client:dev": "cd client && yarn start",
		"dev": "concurrently --kill-others \"yarn run server:dev\" \"yarn run client:dev\""
	},
	"dependencies": {
		"aa-sqlite": "^1.0.29",
		"class-validator": "^0.14.0",
		"express": "^4.18.2",
		"helmet": "^6.0.1",
		"luxon": "^3.2.1",
		"sqlite": "^4.1.2",
		"sqlite3": "^5.1.4"
	}
}
